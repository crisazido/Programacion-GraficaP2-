			float pixelWidth2 =  1.0f / _Vector0.x;
			float pixelHeight2 = 1.0f / _Vector0.y;
			half2 pixelateduv2 = half2((int)(i.uv_texcoord.x / pixelWidth2) * pixelWidth2, (int)(i.uv_texcoord.y / pixelHeight2) * pixelHeight2);
